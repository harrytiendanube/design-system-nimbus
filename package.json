{
  "name": "monorepo-nimbus",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:TiendaNube/design-system-nimbus.git",
  "author": "tiendanube",
  "scripts": {
    "clean": "find . -name node_modules -exec rm -rf '{}' + && rm -rf yarn.lock",
    "start:components": "yarn workspace @tiendanube/components run start",
    "start:storybook": "yarn workspace @tiendanube/storybook run storybook",
    "start:dev": "concurrently --kill-others \"yarn start:components\" \"yarn start:storybook\""
  },
  "npmClient": "yarn",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "contributors": [
    "kelman.celis@tiendanube.com",
    "pablo.radomski@tiendanube.com"
  ],
  "devDependencies": {
    "concurrently": "5.2.0"
  }
}
