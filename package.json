{
  "private": false,
  "name": "@tiendanube/design-system-nimbus",
  "version": "0.10.9",
  "description": "Public design system tiendanube/nimbus",
  "main": "dist/index.js",
  "scripts": {
    "test": "react-scripts test --watchAll=false",
    "test-snap": "react-scripts test Storyshots --watchAll=false",
    "test-unit": "react-scripts test --testPathIgnorePatterns=src/Storyshots.test.js --watchAll=false",
    "test-coverage": "react-scripts test --testPathIgnorePatterns=src/Storyshots.test.js --coverage --watchAll=false",
    "start": "react-scripts start",
    "build": "babel src/components --out-dir dist --copy-files",
    "format": "yarn prettier --write  src/**/*.js ",
    "test:ci": "start-server-and-test storybook http://localhost:9009",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TiendaNube/design-system-nimbus.git"
  },
  "keywords": [
    "Design system",
    "tiendanube",
    "nimbus"
  ],
  "author": "tiendanube",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TiendaNube/design-system-nimbus/issues"
  },
  "homepage": "https://github.com/TiendaNube/design-system-nimbus#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/pro-light-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1",
    "start-server-and-test": "1.10.11"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@storybook/addon-a11y": "5.3.18",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-docs": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addon-storyshots": "5.3.18",
    "@storybook/addon-storyshots-puppeteer": "5.3.18",
    "@storybook/addon-storysource": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/preset-create-react-app": "2.1.1",
    "@storybook/react": "5.3.18",
    "@tiendanube/eslint-config": "^0.2.0",
    "auto": "^9.27.1",
    "babel-loader": "8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "husky": "4.2.5",
    "prettier": "2.0.4",
    "pretty-quick": "2.0.1",
    "puppeteer": "2.1.1",
    "react": "16.13.1",
    "react-is": "16.13.1",
    "react-test-renderer": "16.13.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/serviceWorker.js",
      "<rootDir>/src/index.js",
      "<rootDir>/src/Storyshots.test.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=8",
    "yarn": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "yarn test:ci"
    }
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/env"
    ]
  }
}
